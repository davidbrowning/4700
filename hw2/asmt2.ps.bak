%!PS
/printthing
{
    /word exch def
    (Arial)
    findfont
    24
    scalefont
    setfont
    % X then Y
    150 550
    moveto
    word
    .8 .30 .20
    setrgbcolor
    show
} def


/circxy
{
    /t exch def
    t t sin mul
    t t cos mul
}
def

/xt 
{
    /t exch def
    a b sub t cos mul a b sub t mul b div cos d mul
} def

/yt 
{
    /t exch def
    a b sub t sin mul a b sub t mul b div sin d mul
} def

/inch
{ 72 mul } def
/center
{ 
/one exch def
/two exch def
4 inch one add 5.5 inch two add
} def

/F
{
/x exch def
/y exch def
gsave
x 0 gt {
x y center moveto
y 0 rlineto
0 x rlineto
stroke
newpath
x y center moveto
0 x -1 mul rlineto
stroke
/x x 2 div def
/y y 2 div def
y x F
    
} if
} def


newpath
4 inch 5.5 inch moveto
1 inch 1 inch F

newpath
