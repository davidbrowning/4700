%stroke

/go_right {
  /x exch def
  /y exch def
  -45 rotate
  x y rlineto
  stroke
} def

/go_left {
  /x exch def
  /y exch def
  45 rotate
  x y rlineto
  stroke
} def

/45fork {
  /d exch def %depth
  /x exch def
  /y exch def
  d 0 gt 
  {
    d 1 eq 
    {
      stroke
      grestore
%      exit
    } if
    gsave
    45 rotate
    x y rlineto 
    y x d 1 sub 45fork
    -45 rotate
    x y rlineto
    y x d 1 sub 45fork

  } if
}def

1 inch 1 inch 7 45fork




